(declare-const X_0 Real)
(declare-const X_1 Real)
(declare-const X_2 Real)

(declare-const Y_0 Real)

(assert (<= (* -1.0 X_0) 0.0))
(assert (<= X_0 50.0))
(assert (<= (* -1.0 X_1) 50.0))
(assert (<= X_1 50.0))
(assert (<= (* -1.0 X_2) 0.0))
(assert (<= X_2 150.0))
(assert (<= (- (* 1.5 X_1) X_2) -5.0))

(assert (or
    (<= Y_0 0.0)
    (>= Y_0 1.0)
))